.register .form .label::after {
  content: "";
  bottom: 0;
  width: 100%;
  height: 1px;
  background-color: #999;
  position: absolute;
}
.register .form .label {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  padding: 24rpx 30rpx;
  position: relative;
}
.register .form .label .text {
    width: 30%;
}
.register .form .label .data {
    width: 70%;
}
.register .btns {
  padding: 24rpx 30rpx;
}
.register .btns .goLogin {
    font-size: 28rpx;
    color: #3893f3;
    margin-top: 20rpx;
}

