<view class="approvalNotice" bind:__l="__l"><block wx:if="{{dataInfo.length===0}}"><view class="content-no"><image class="img" src="../../../static/img/content-null.png"></image><view class="text">暂无任何审批通知</view></view></block><block wx:else><view class="content-has"><block wx:for="{{dataInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view class="item-datetime">{{item.applay_date}}</view><view class="item-main"><view class="vimg"><image class="img" src="../../../static/img/message_icon_01.png" mode></image></view><view class="text"><view class="text-t">待审信息</view><view class="item-main-main"><view class="item-main-main-1"><view class="item-main-main-1-l"><view class="item-main-main-1-l-time">{{item.time}}</view></view><view class="item-main-main-1-r">{{item.date}}</view></view><view class="item-main-main-2"><text>标题 :</text><text>{{''+item.title}}</text></view><view class="item-main-main-3"><text>业务名称 :</text><text>{{''+item.bus_type}}</text></view><view class="item-main-main-3"><text>开始办理时间 :</text><text>{{''+item.applay_date}}</text></view><view class="item-main-main-4"><text>发起人 :</text><text>{{''+item.name}}</text></view><view class="item-main-main-5">{{''+item.state+''}}</view></view></view></view></view></block></view></block></view>