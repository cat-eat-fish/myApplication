<view class="financialTrial" bind:__l="__l"><view class="financialTrial-form"><view class="item"><view class="text">业务名称 :</view><view class="field">{{''+dataInfo.name+''}}</view></view><view class="item"><view class="text">业务机构 :</view><view class="field">{{''+dataInfo.jg+''}}</view></view><view class="item"><view class="text">发起部门 :</view><view class="field">{{''+dataInfo.fqbm+''}}</view></view><view class="item"><view class="text">申购项目 :</view><view class="field"><input type="text" value="{{dataInfo.sgxm}}" placeholder="请输入申购项目" data-event-opts="{{[['input',[['__set_model',['$0','sgxm','$event',[]],['dataInfo']]]]]}}" bindinput="__e"/></view></view><view class="item"><view class="text">摘要 :</view><view class="field"><input type="text" value="{{dataInfo.msg}}" placeholder="请输入摘要" data-event-opts="{{[['input',[['__set_model',['$0','msg','$event',[]],['dataInfo']]]]]}}" bindinput="__e"/></view></view><view class="item"><view class="text">金额 :</view><view class="field"><uni-number-box min="{{0}}" max="{{9999999}}" value="{{dataInfo.money}}" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e"></uni-number-box></view></view><view class="item"><view class="text">资料上传</view><input disabled type="text" placeholder="附件上传" data-event-opts="{{[['tap',[['openFile',['$event']]]]]}}" bindtap="__e"/><tki-file-manager data-ref="filemanager" data-event-opts="{{[['^result',[['resultPath']]]]}}" class="vue-ref" bind:result="__e"></tki-file-manager></view><block wx:if="{{path.length!=0}}"><view class="item"><view class="text">资料列表</view><view class="textarea"><block wx:for="{{path}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item-list"><view class="desc">{{item}}</view><view class="del" data-value="{{index}}" data-event-opts="{{[['tap',[['delList',['$event']]]]]}}" bindtap="__e">删除</view></view></block></view></view></block></view><view class="btns"><button type="warn" data-event-opts="{{[['tap',[['doUpload',['$event']]]]]}}" bindtap="__e">附件上传</button><tki-file-manager data-ref="filemanager" data-event-opts="{{[['^result',[['resultPath']]]]}}" class="vue-ref" bind:result="__e"></tki-file-manager><button type="primary" data-event-opts="{{[['tap',[['submitAcce',['$event']]]]]}}" bindtap="__e">提交受理</button></view></view>